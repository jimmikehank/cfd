FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Tinlet          298;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinlet;

boundaryField
{
    meanFlow
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }

    down
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }

    up
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }

    right
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }



    surface
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }

    interface1
    {
        type            empty;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}
