FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 100000;

boundaryField
{
    inOut
    {
        type            fixedValue;
        value           uniform 100000;
    }
    surface
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


