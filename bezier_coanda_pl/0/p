/*---------------------------------*- C++ -*-----------------------------------*/

FoamFile
{
		version			2.0;
		format			ascii;
		class				volScalarField;
		object			p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions			[1 -1 -2 0 0 0 0];

internalField		uniform 100000;
upper_pressure uniform 100000.0;						 // Upper Plenum Pressure
lower_pressure uniform 100001;						 // Lower Plenum Pressure

boundaryField
{
		inOut
		{
				type						fixedValue;
				value						$internalField;
		}

		surface
		{
				type						zeroGradient;
		}
    surfInner
    {
        type		zeroGradient;
    }

		coandaUpper
		{
				// type zeroGradient;
				type 						fixedValue;
				value						$upper_pressure;
		}
		coandaLower
		{
				// type zeroGradient;
				type 						fixedValue;
				value						$lower_pressure;
		}

		defaultFaces
		{
				type						empty;
		}
}

// ************************************************************************* //
