forces
{
    type          forces;
    libs          ("libforces.so");

    writeControl  timeStep;
    writeInterval  3000;

    log           no;

    patches       ("surface" "coandaUpper" "coandaLower");
    rho           rho;
    log           true;

    CofR          (-0.225 0 0);    // Rotation around centre line of propeller
}


