FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    down
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    up
    {
        type            zeroGradient;
    }
    meanFlow
    {
        type            uniformFixedValue;
        uniformValue    
        {
            type            constant;
            value           (20 0 0);
        }
        value           uniform (20 0 0);
    }
    coandaUpper
    {
        type            flowRateInletVelocity;
        massFlowRate    
        {
            type            constant;
            value           0;
        }
        value           nonuniform List<vector> 0();
    }
    coandaLower
    {
        type            flowRateInletVelocity;
        massFlowRate    
        {
            type            constant;
            value           0.005;
        }
        value           nonuniform List<vector> 0();
    }
    surface
    {
        type            noSlip;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


