FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U.step;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (35 0 0);

boundaryField
{
    down
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    up
    {
        type            zeroGradient;
    }
    meanFlow
    {
        type            uniformFixedValue;
        uniformValue    
        {
            type            constant;
            value           (35 0 0);
        }
        value           uniform (35 0 0);
    }
    coandaUpper
    {
        type            flowRateInletVelocity;
        massFlowRate    
        {
            type            scale;
            scale           
            {
                type            constant;
                value           0.006;
            }
            xScale          
            {
                type            constant;
                value           1;
            }
            value           
            {
                type            squarePulse;
                start           0.2;
                duration        0.00086;
            }
        }
        value           nonuniform List<vector> 0();
    }
    coandaLower
    {
        type            noSlip;
    }
    surface
    {
        type            noSlip;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform (35 0 0);
    }
    procBoundary2to5
    {
        type            processor;
        value           uniform (35 0 0);
    }
}


