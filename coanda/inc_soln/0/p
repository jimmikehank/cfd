FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    down
    {
        type            symmetryPlane;
    }

    right
    {
        type            fixedValue;
        value           uniform 0;
    }

    up
    {
        type            symmetryPlane;
    }

    meanFlow
    {
        type            zeroGradient;
    }

    surface
    {
        type            zeroGradient;
    }
    coandaInlet
    {
        type            fixedValue;
        value           uniform 70000;
    }

    defaultFaces
    {
        type            empty;
    }
}


