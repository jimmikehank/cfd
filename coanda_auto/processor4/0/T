FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    down
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    up
    {
        type            zeroGradient;
    }
    meanFlow
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    surface
    {
        type            zeroGradient;
    }
    surfaceInner
    {
        type            zeroGradient;
    }
    coandaInlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary4to1
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 298;
    }
}


