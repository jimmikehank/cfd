FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    down
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    right
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    up
    {
        type            calculated;
        value           uniform 0;
    }
    meanFlow
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    surface
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    surfaceInner
    {
        type            compressible::alphatWallFunction;
        value           nonuniform List<scalar> 0();
    }
    coandaInlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary4to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 0;
    }
}


