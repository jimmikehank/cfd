FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.014;

boundaryField
{
    down
    {
        type            freestream;
        freestreamValue uniform 0.014;
        value           uniform 0.014;
    }
    right
    {
        type            freestream;
        freestreamValue nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    up
    {
        type            freestream;
        freestreamValue nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    meanFlow
    {
        type            freestream;
        freestreamValue nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    surface
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    surfaceInner
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    coandaInlet
    {
        type            freestream;
        freestreamValue uniform 0.014;
        value           uniform 0.014;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.014;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.014;
    }
    procBoundary1to4
    {
        type            processor;
        value           uniform 0.014;
    }
}


