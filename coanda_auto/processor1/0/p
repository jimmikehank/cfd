FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    down
    {
        type            fixedValue;
        value           uniform 100000;
    }
    right
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    up
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    meanFlow
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    surface
    {
        type            zeroGradient;
    }
    surfaceInner
    {
        type            zeroGradient;
    }
    coandaInlet
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary1to4
    {
        type            processor;
        value           uniform 100000;
    }
}


