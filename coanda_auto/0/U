/*---------------------------------*- C++ -*-----------------------------------*/

FoamFile
{
		version			2.0;
		format			ascii;
		class				volVectorField;
		location		"0";
		object			U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mfs 						25;
mdot					0.0055;

dimensions			[0 1 -1 0 0 0 0];

internalField		uniform ($mfs  0  0);

boundaryField
{
		down
		{
				type						zeroGradient;
		}

		right
		{
				type						zeroGradient;
		}

		up
		{
				type						zeroGradient;
		}

		meanFlow
		{
				type						uniformFixedValue;
				uniformValue		constant ($mfs  0  0);
				value						uniform ($mfs  0  0);
		}

		surface
		{
				type						noSlip;
		}

		surfaceInner
		{
				type						noSlip;
		}

		coandaInlet
		{
				type						flowRateInletVelocity;
				massFlowRate		$mdot;
				value						uniform ($mdot 0 0);
		}

		defaultFaces
		{
				type						empty;
		}
}

// ************************************************************************* //
