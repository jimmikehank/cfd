FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Tinlet          298;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinlet;

boundaryField
{
		inOut
		{
				type		inletOutlet;
				inletValue			uniform $Tinlet;
				value			$inletValue;
		}
		surface
		{
				type		zeroGradient;
		}
		coandaLower
		{
				type		zeroGradient;
		}
		coandaUpper
		{
				type		inletOutlet;
				inletValue			uniform $Tinlet;
				value			$inletValue;
		}
		defaultFaces
		{
				type			empty;
		}
		#includeEtc "caseDicts/setConstraintTypes"
}