FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField uniform (10 0 0);

boundaryField
{
    "(up|down|left|right)"
    {
        type            noSlip;
    }

    inlet
    {
        type                turbulentInlet;
        fluctuationScale    (0.1 0.1 0.1);
        referenceField      $internalField;
        alpha               0.1;
    }

    outlet
    {
        type            advective;
        value           $internalField;
    }

}


// ************************************************************************* //
