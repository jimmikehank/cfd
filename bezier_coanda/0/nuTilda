FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

def_pl          uniform 4.5e-5;
coanda_exit     uniform 1.4e-1;

internalField   $def_pl;


boundaryField
{
    meanFlow
    {
        type            inletOutlet;
        inletValue      $def_pl;
        value           $def_pl;
    }

    right
    {
        type            inletOutlet;
        inletValue      $def_pl;
        value           $def_pl;
    }

    down
    {
        type            inletOutlet;
        inletValue      $def_pl;
        value           $def_pl;
    }

    up
    {
        type            inletOutlet;
        inletValue      $def_pl;
        value           $def_pl;
    }
    surface
    {
        type            fixedValue;
        value           uniform 0.0;
    }
    coandaUpper
    {
        type            fixedValue;
        value           uniform 0.0;
    }
    coandaLower
    {
        type            inletOutlet;
        inletValue      $def_pl;
        value           $def_pl;
    }
}

// ************************************************************************* //
