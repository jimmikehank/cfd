// STEADY STATE BLOWING
FoamFile
{
		version			2.0;
		format			ascii;
		class				volVectorField;
		location		"0";
		object			U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

alpha						0;						// AoA in Degrees
U 							25; 					// Flow magnitude
massflow				0.006;				 // mass flow rate


dimensions			[0 1 -1 0 0 0 0];

internalField		uniform  (#calc "$U * cos(degToRad($alpha))" #calc "$U*sin(degToRad($alpha))"  0);


boundaryField
{
		inOut
		{
				type            inletOutlet;
				inletValue      $internalField;
				value           $internalField;
		}
		surface
		{
				type            fixedValue;
				value           uniform (0 0 0);
		}

		coandaUpper
		// COANDA STEADY:
		{
				type						flowRateInletVelocity;
				massFlowRate		$massflow;
				value						uniform ($massflow 0 0);
		}

		coandaLower
		{
				type            fixedValue;
				value           uniform (0 0 0);
		}


		defaultFaces
		{
				type						empty;
		}
}

// ************************************************************************* //
