FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

def_pl          uniform 4.5e-5;
coanda_exit     uniform 1.4e-1;

internalField   $def_pl;

boundaryField
{
    meanFlow
    {
        type            calculated;
        value           $def_pl;
    }

    right
    {
        type            calculated;
        value           $def_pl;
    }

    down
    {
        type            calculated;
        value           $def_pl;
    }

    up
    {
        type            calculated;
        value           $def_pl;
    }

    surface
    {
        type            nutUSpaldingWallFunction;
        value           $def_pl;
    }
    coandaUpper
    {
        type            calculated;
        value           uniform 0.0;
    }

    coandaLower
    {
        type            calculated;
        value           $def_pl;
    }
}

// ************************************************************************* //
