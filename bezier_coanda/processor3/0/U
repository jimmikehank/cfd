FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (70 0 0);

boundaryField
{
    inOut
    {
        type            inletOutlet;
        inletValue      uniform (70 0 0);
        value           uniform (70 0 0);
    }
    coandaUpper
    {
        type            flowRateInletVelocity;
        massFlowRate    
        {
            type            constant;
            value           0.010554;
        }
        value           uniform (0.010554 0 0);
    }
    coandaLower
    {
        type            flowRateInletVelocity;
        massFlowRate    
        {
            type            constant;
            value           0;
        }
        value           nonuniform List<vector> 0();
    }
    surface
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform (70 0 0);
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform (70 0 0);
    }
}


