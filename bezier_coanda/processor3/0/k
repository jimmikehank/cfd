FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.02;

boundaryField
{
    inOut
    {
        type            inletOutlet;
        inletValue      uniform 0.02;
        value           uniform 0.02;
    }
    coandaUpper
    {
        type            inletOutlet;
        inletValue      uniform 0.02;
        value           uniform 0.02;
    }
    coandaLower
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    surface
    {
        type            kqRWallFunction;
        value           uniform 0.02;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 0.02;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0.02;
    }
}


