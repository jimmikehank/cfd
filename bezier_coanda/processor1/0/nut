FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 4.5e-05;

boundaryField
{
    inOut
    {
        type            calculated;
        value           uniform 4.5e-05;
    }
    coandaUpper
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    coandaLower
    {
        type            calculated;
        value           uniform 4.5e-05;
    }
    surface
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 4.5e-05;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 4.5e-05;
    }
}


