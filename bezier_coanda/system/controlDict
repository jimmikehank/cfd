FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoSimpleFoam;

startFrom        latestTime;

startTime       0;

stopAt           endTime;

endTime         2000;

deltaT          1;

writeControl     timeStep;

    div(phi,U)      bounded Gauss linear;
    div(div(phi,U)) Gauss linear;

writeInterval   2000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
}



